extends layout

block content
  .limiter
    .container-login100
      .wrap-login100.p-l-55.p-r-55.p-t-65.p-b-50
        form.login100-form.validate-form(method='post', action='/findname')
          span.login100-form-title.p-b-33
            | Search for Data
          .wrap-input100.validate-input
            input.input100(type='text', name='firstName', placeholder='First Name', required='')
            span.focus-input100-1
            span.focus-input100-2
          .wrap-input100.rs1.validate-input
            input.input100(type='text', name='lastName', placeholder='Last Name', required='')
            span.focus-input100-1
            span.focus-input100-2
          .container-login100-form-btn.m-t-20
            button.login100-form-btn(type='submit', value='Add Name')
              | Search 
